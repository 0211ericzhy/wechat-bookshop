}

.bigbox {
  margin: 0px 20rpx;
  width: 200rpx;
  height: 300rpx;
  /* background-color: pink; */
  font-size: 14rpx;
  text-align: center;
}

image {
  width: 200rpx;
  height: 250rpx;
}

.ffont {
  /* color: #eeeeee; */
  font-size: 12rpx;
}

.moves {
  animation: move 2s 0s infinite;
  position: relative;
}

@keyframes move {


  0% {
    -webkit-transform: rotate(0deg);
    transform: rotate(0deg);
  }

  25% {
    -webkit-transform: rotate(1deg);
    transform: rotate(1deg);
  }

  50% {
    -webkit-transform: rotate(-1deg);
    transform: rotate(-1deg);
  }

  75% {
    -webkit-transform: rotate(1deg);
    transform: rotate(1deg);
  }

  100% {
    -webkit-transform: rotate(-1deg);
    transform: rotate(-1deg);

  }
}
.imgs{
  position: absolute;
  top: 1rpx;
  left: 1rpx;
  width: 30rpx;
  height: 30rpx;
}